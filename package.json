{
  "name": "laurendorman",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nextein && npm run build:watch",
    "export": "nextein build && nextein export",
    "deploy": "npx now alias --token=$NOW_TOKEN $(npx now ./out --static --no-clipboard --token=$NOW_TOKEN --public) laurendorman.io",
    "mutations": "immutable-css src/tachyons.css --strict",
    "build": "npm run build:css && npm run build:minify",
    "build:css": "tachyons src/tachyons.css > static/css/styles.css",
    "build:minify": "tachyons src/tachyons.css -m > static/css/styles.min.css",
    "build:watch": "watch \"npm run build\" ./src/"
  },
  "keywords": [],
  "author": "Lauren Dorman",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^1.29.0",
    "fontfaceobserver": "^2.0.13",
    "highlight.js": "^9.12.0",
    "next": "^5.0.0",
    "nextein": "^1.2.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.1",
    "remark": "^9.0.0",
    "remark-react": "^4.0.1",
    "remark-react-lowlight": "^0.7.0"
  },
  "devDependencies": {
    "copy-files": "^0.1.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "immutable-css-cli": "^1.1.1",
    "normalize.css": "^8.0.0",
    "tachyons-cli": "^1.2.0",
    "tachyons-modules": "^1.1.10",
    "watch": "^1.0.2"
  }
}
